{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    
    "rules": {
        "no-unused-vars": "warn",
        "no-trailing-spaces":"warn",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
    }
}